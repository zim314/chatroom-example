{
  "compilerOptions": {
      "target": "ESNext",
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true, 
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "noImplicitAny": true, 
      "strictNullChecks": true, 
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "baseUrl": "src",
  },
}
